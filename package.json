{
  "name": "kilogram",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "admin-on-rest": "^1.3.3",
    "aor-epilogue-client": "^0.2.0",
    "aor-rest-client-router": "^0.1.0",
    "bluebird": "^3.5.1",
    "bson": "^1.0.4",
    "child-process-promise": "^2.2.1",
    "config": "^1.28.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "finale-rest": "^0.1.3",
    "lodash": "^4.17.4",
    "minio": "^3.2.3",
    "msgpack-lite": "^0.1.26",
    "nodemon": "^1.14.3",
    "passport-minitools": "https://github.com/robbywashere/passport-minitools",
    "pg": "^6.4.2",
    "python-shell": "^0.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.0.17",
    "sequelize": "^4.28.6",
    "sequelize-encrypted": "^1.0.0",
    "stream-to-promise": "^2.2.0",
    "supertest": "^3.0.0",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "dev:tabs": "./scripts/tab npm run minio:up & ./scripts/tab npm run dev & ./scripts/tab npm run crx:start",
    "crx:start": "export PORT=3001 && react-scripts start",
    "crx:build": "react-scripts build",
    "crx:test": "react-scripts test --env=jsdom",
    "crx:eject": "react-scripts eject",
    "minio:up": "minio server \"$HOME/minio-data/\"",
    "minio:bucket:up": "mc mb myminio/uploads",
    "minio:events:up": "mc events add myminio/uploads arn:minio:sqs::1:postgresql --events put,delete",
    "minio:events:down": "mc events remove myminio/uploads arn:minio:sqs::1:postgresql --force",
    "dev:auto": "NODE_ENV=development && nodemon server.js",
    "dev": "NODE_ENV=development && node server.js",
    "eslint": "eslint",
    "sync:seed": " node ./db/_sync.js && node ./db/seeders/_up.js",
    "seeds:up": "node ./db/seeders/_up.js",
    "seeds:down": "node ./db/seeders/_down.js",
    "db:sync": "node ./db/_sync.js",
    "db:up": "node ./db/_up.js",
    "db:down": "node ./db/_down.js",
    "db:mig:down": "node ./db/_migdown.js",
    "db:mig:up": "node ./db/_migup.js",
    "test": "supervisor -q -n exit -x mocha -- -b ./tests/*.test.js"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "sinon": "^4.1.3",
    "stream-array": "^1.1.2"
  }
}
